    <title>Upload File using Multer</title>
  </head>
  <body>
    <main class="admin">
      <h1>Upload File</h1>
      <hr />
      <div>
        <form action="/uploadFile" enctype="multipart/form-data" method="POST">
          <input type="file" id="myFile" name="myFile" />
          <br />
          <br />
          <input type="submit" />
        </form>
      </div>
    </main>
  </body>
